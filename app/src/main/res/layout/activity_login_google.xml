<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nextactivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="261dp"
        android:layout_height="37dp"
        android:layout_marginStart="74dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="91dp"
        android:text="Welcome to Google Log in "
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#0C0C0C"
        app:layout_constraintBottom_toTopOf="@+id/nameG"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42000002" />

    <TextView
        android:id="@+id/nameG"
        android:layout_width="259dp"
        android:layout_height="51dp"
        android:layout_marginStart="69dp"
        android:layout_marginTop="91dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="18dp"
        android:text="name "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/emailG"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/emailG"
        android:layout_width="257dp"
        android:layout_height="52dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="10dp"
        android:text="Email"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#1B1919"
        app:layout_constraintBottom_toTopOf="@+id/but_singout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameG" />

    <Button
        android:id="@+id/but_singout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="292dp"
        android:backgroundTint="@color/primary"
        android:text="Sing Out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailG" />
</androidx.constraintlayout.widget.ConstraintLayout>